{
  "name": "cmdrsharp/netbox-laravel-bgp",
  "description": "BGP Session Resource for netbox-laravel",
  "type": "libr",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.4",
    "cmdrsharp/netbox-laravel": ">=1.0.0.0"
  },
  "require-dev": {
    "phpunit/phpunit":">=7.0",
    "orchestra/testbench": ">=6.0",
    "ext-json": "*"
  },
  "prefer-stable": true,
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "CmdrSharp\\NetBoxBgp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CmdrSharp\\NetBox\\Tests\\": "vendor/cmdrsharp/netbox-laravel/tests/",
      "CmdrSharp\\NetBoxBgp\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
